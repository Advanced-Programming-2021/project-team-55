<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<?import java.lang.*?>
<AnchorPane prefHeight="720" prefWidth="1080" stylesheets="@../CSS/Menu.css" xmlns="http://javafx.com/javafx/11.0.1"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="yugioh.controller.menucontroller.WelcomeMenuController">
    <Label layoutX="387.0" layoutY="165.0" stylesheets="@../CSS/Menu.css" text="YU GI OH" textFill="#e1ff00">
        <font>
            <Font name="Bauhaus 93" size="78.0"/>
        </font>
    </Label>
    <VBox alignment="CENTER" layoutX="309.0" layoutY="226.0" maxHeight="720.0" maxWidth="1080.0" prefHeight="267.0"
          prefWidth="462.0" spacing="10.0" AnchorPane.bottomAnchor="227.0" AnchorPane.leftAnchor="309.0"
          AnchorPane.rightAnchor="309.0" AnchorPane.topAnchor="226.0">
        <Button defaultButton="true" mnemonicParsing="false" onMouseClicked="#enterLoginMenuClicked" prefHeight="58.0"
                prefWidth="272.0" text="Login">
            <styleClass>
                <String fx:value="buttonShape"/>
                <String fx:value="buttonText"/>
            </styleClass>
        </Button>
        <Button alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false"
                onMouseClicked="#enterRegisterMenuClicked" prefHeight="58.0" prefWidth="272.0" text="Register">
            <styleClass>
                <String fx:value="buttonShape"/>
                <String fx:value="buttonText"/>
            </styleClass>
        </Button>
        <Button alignment="CENTER" mnemonicParsing="false" onMouseClicked="#exitClicked" prefHeight="58.0"
                prefWidth="272.0" text="Exit">
            <styleClass>
                <String fx:value="buttonText"/>
                <String fx:value="buttonShape"/>
            </styleClass>
        </Button>
    </VBox>
</AnchorPane>
