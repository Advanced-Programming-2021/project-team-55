<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="119.0" prefWidth="400.0" stylesheets="@../CSS/Menu.css" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="yugioh.controller.menucontroller.SelectDeckNamePageController">
   <children>
      <VBox alignment="CENTER" prefHeight="200.0" prefWidth="400.0" spacing="10.0">
         <children>
            <Label styleClass="popupText" text="Please enter a name for deck" />
            <TextField fx:id="deckNameField" />
            <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
               <children>
                  <Button fx:id="selectionOkButton" mnemonicParsing="false" text="Ok" />
                  <Button fx:id="selectionBackButton" mnemonicParsing="false" text="Back" />
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
